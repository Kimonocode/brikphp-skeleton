{
    "name": "kimonocode/brikphp-skeleton",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "App\\Tests\\" : "tests/"
        }
    },
    "require": {
        "kimonocode/brikphp": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}
